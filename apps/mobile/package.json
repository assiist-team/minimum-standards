{
  "name": "MinimumStandardsMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "concurrently \"npm start\" \"react-native run-android\"",
    "ios": "concurrently \"npm start\" \"react-native run-ios\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:functions": "npm --prefix ../../packages/firestore-rules-tests test",
    "test:detox:ios": "detox test --configuration ios.sim.debug",
    "test:detox:android": "detox test --configuration android.emu.debug",
    "test:detox": "npm run test:detox:ios && npm run test:detox:android",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@minimum-standards/firestore-model": "file:../../packages/firestore-model",
    "@minimum-standards/shared-model": "file:../../packages/shared-model",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native-firebase/auth": "^23.7.0",
    "@react-native-firebase/crashlytics": "^23.7.0",
    "@react-native-firebase/firestore": "^23.7.0",
    "@react-native-firebase/messaging": "^23.7.0",
    "@react-native-google-signin/google-signin": "^13.3.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "lodash.debounce": "^4.0.8",
    "luxon": "^3.7.2",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.5",
    "react-native-config": "^1.6.1",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-safe-area-context": "4.10.8",
    "react-native-screens": "~3.35.0",
    "react-native-vector-icons": "^10.2.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^15.0.0",
    "@react-native-community/cli-platform-android": "^15.0.0",
    "@react-native-community/cli-platform-ios": "^15.0.0",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.3.3",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.7",
    "concurrently": "^9.1.0",
    "detox": "^20.16.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.1",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
